<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="cpp.quiz.sfedu.cppquiz">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/my_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/my_launcher"
            android:supportsRtl="true"
            android:theme="@style/AppTheme" tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".main.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>


        <activity
                android:exported="true"
                android:parentActivityName=".main.activity.MainActivity"
                android:name=".testMenu.activity.TestMenuActivity">
        </activity>

        <activity
                android:name=".test.activity.TestActivity"
                  android:exported="true">
        </activity>


    </application>
</manifest>